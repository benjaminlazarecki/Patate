{
    "name": "patate/patate-command",
    "description": "Provide some commands to execute phpcs on github repository",
    "keywords": [ "patate", "command", "phpcs", "github", "pull-request" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Benjamin Lazarecki",
            "email": "benjamin.lazarecki@gmail.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "Patate": "src"
        }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/console": "dev-master",
        "knplabs/github-api": "*",
        "pimple/pimple": "dev-master",
        "symfony/process": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "target-dir": "Patate/PatateCommand",
    "minimum-stability": "dev"
}
